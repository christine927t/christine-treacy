gin-right: 10px;\r\n    padding-top: 300px;\r\n}\r\n\r\n.about-heading span{\r\n    color: transparent;\r\n\t-webkit-text-stroke: .75px rgb(228, 194, 5);\r\n\ttext-stroke: 1px rgb(228, 194, 5);\r\n\t-webkit-text-fill-color: transparent;\r\n\ttext-fill-color: transparent;\r\n}\r\n\r\nh2 {\r\n    font-size: 2.5em;\r\n}\r\n\r\n.about {\r\n    font-size: 30px;\r\n    text-align: right;\r\n}\r\n\r\n\r\nimg {\r\n    display: block;\r\n    margin: 100px auto 0 auto;\r\n    height: 400px;\r\n    width: 400px;\r\n    padding: 250px 0 10px 0;\r\n}\r\n\r\nnav {\r\n    height: 100px;\r\n}\r\n\r\nnav ul{\r\n    list-style-type: none;\r\n    padding-left: 10px;\r\n}\r\nnav ul li {\r\n    display: inline-block;\r\n    font-size: 24px;\r\n}\r\n\r\na {\r\n    text-decoration: none;\r\n    color:white;\r\n}\r\n\r\n.nav-about {\r\n    padding-right: 7px;\r\n}\r\n\r\n.nav-trade {\r\n    padding-right: 7px;\r\n}\r\n\r\n.nav-left {\r\n    float: left;\r\n}\r\n.nav-middle{\r\n    float: right;\r\n    padding-right: 10px;\r\n    align-content: space-between;\r\n} \r\n\r\n.nav-right{\r\n    float: right;\r\n    padding-right: 70px;\r\n}\r\n    \r\n/* locomotive-scroll */\r\nsection {\r\n    height: 100vh;\r\n    color: white;    \r\n} \r\n\r\nsection h1 {\r\n        font-size: 3em;\r\n}\r\n\r\nsection p {\r\n        font-size: 1.5em;\r\n}\r\n\r\n/* diagonal box */\r\n.diagonal-box1, .diagonal-box2 {\r\n    position: relative;\r\n}\r\n\r\n.diagonal-box1:before, .diagonal-box2:before {\r\n    position: absolute;\r\n    top: 0;\r\n    right: 0;\r\n    left: 0;\r\n    bottom: 0;\r\n    height: 100vh;\r\n    color:white;\r\n    /* skews section along the y-axis to create diagonal effect */\r\n    transform: skewY(-32deg);\r\n    content: '';\r\n    margin: 0;\r\n    padding: 0;\r\n}\r\n\r\n.diagonal-box1:before{\r\n    background: #993380; \r\n}\r\n\r\n.diagonal-box2:before{\r\n    background: #003399; \r\n}\r\n\r\n.content {\r\n    max-width: 50em;\r\n    margin: 0 auto;\r\n    text-align: center;\r\n    position: relative;\r\n}\r\n\r\n.all-titles {\r\n    position: absolute;\r\n    bottom: 600px;\r\n    right: 10px;\r\n}\r\n\r\n.fas {\r\n    position: absolute;\r\n    bottom: 200px;\r\n    left: 50%;\r\n    color:rgb(182, 182, 174)\r\n}\r\n.titles-p {\r\n    opacity: 0;\r\n    transition: 3s;\r\n    color: rgb(182, 182, 174);\r\n    font-size: 100px;\r\n    text-align: right;\r\n    list-style: none;\r\n    padding: 5px;\r\n    display: block;\r\n}\r\n.project-p {\r\n    opacity: 0;\r\n    transition: 6s;\r\n    color: rgb(182, 182, 174);\r\n    padding-top: 1px;\r\n    \r\n}\r\n\r\n.scroll-class {\r\n    opacity: 1;\r\n}\r\n\r\n.is-inview {\r\n    /* color: black; */\r\n}\r\n\r\n/*** flipping card transition ***/\r\n.card {\r\n    position: relative;\r\n    display: inline-block;\r\n    /* float: left; */\r\n    padding-bottom: 25%;\r\n    width: 25%;\r\n    text-align: center;\r\n}\r\n\r\nfooter {\r\n    height: 100px;\r\n}\r\n\r\n/****Media Queries****/\r\n/* @media (max-width: 1507px){\r\n    h1 {\r\n        font-size: 150px;\r\n        letter-spacing: -10px;\r\n    }\r\n}\r\n\r\n@media (max-width: 1123px){\r\n    h1 {\r\n        font-size: 120px;\r\n        letter-spacing: -8px;\r\n    }\r\n\r\n    p {\r\n        line-height: normal;\r\n    }\r\n}\r\n\r\n@media (max-width: 876px){\r\n    h1 {\r\n        font-size: 90px;\r\n        word-wrap: break-word;\r\n        line-height: normal;\r\n        letter-spacing: -5px;\r\n\r\n    }\r\n} */\r\n"},"lineCount":242}},"error":null,"hash":"533eccb356b2317672bb84c75b8f5ed3","cacheData":{"env":{}}}